         <font>
            <Font name="Impact" size="34.0" />
         </font>
      </Button>
      <Button fx:id="playButton" layoutX="242.0" layoutY="646.0" mnemonicParsing="false" onAction="#playMedia" text="PLAY">
         <font>
            <Font name="Impact" size="34.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="549.0" layoutY="646.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET">
         <font>
            <Font name="Impact" size="34.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
